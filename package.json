{
  "browserify": {
    "transform": [
      "envify",
      "unassertify",
      "es2020",
      "uglifyify"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.7",
    "@fortawesome/fontawesome-free-regular": "^5.0.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.12",
    "autosize": "^4.0.0",
    "classnames": "^2.2.5",
    "clipboard": "^1.7.1",
    "dagre": "^0.8.2",
    "debug": "^3.1.0",
    "diff": "^3.5.0",
    "domain-singleton": "^1.0.1",
    "duplexify": "^3.5.3",
    "fast-json-stable-stringify": "^2.0.0",
    "file-saver": "^1.3.3",
    "flush-write-stream": "^1.0.2",
    "from2-array": "0.0.4",
    "inherits": "^2.0.3",
    "is-sha-256-hex-digest": "^1.0.0",
    "length-prefixed-stream": "^1.5.1",
    "moment": "^2.22.1",
    "multistream": "^2.1.0",
    "nanomorph": "^5.1.3",
    "nanoraf": "^3.0.1",
    "page-bus": "^3.0.1",
    "prosemirror-commands": "^1.0.7",
    "prosemirror-dropcursor": "^1.0.0",
    "prosemirror-gapcursor": "^1.0.2",
    "prosemirror-history": "^1.0.2",
    "prosemirror-menu": "^1.0.4",
    "prosemirror-model": "^1.4.4",
    "prosemirror-schema-list": "^1.0.1",
    "prosemirror-state": "^1.2.0",
    "prosemirror-view": "^1.3.0",
    "pump": "^3.0.0",
    "pumpify": "^1.5.0",
    "rfc6902-json-diff": "^0.2.1",
    "run-parallel": "^1.1.9",
    "run-series": "^1.1.8",
    "signalhub": "^4.9.0",
    "simple-get": "^2.7.0",
    "sodium-javascript": "^0.5.5",
    "through2": "^2.0.3",
    "webrtc-swarm": "^2.9.0"
  },
  "devDependencies": {
    "ajv": "^5.3.0",
    "browserify": "^16.2.0",
    "browserify-header": "^0.9.4",
    "chromedriver": "^2.38.3",
    "common-shakeify": "^0.4.5",
    "envify": "^4.1.0",
    "es2020": "^1.1.9",
    "exorcist": "^1.0.1",
    "husky": "^0.14.3",
    "licensee": "^4.5.0",
    "lint-staged": "^7.0.5",
    "node-sass": "^4.7.1",
    "npm-run-all": "^4.1.2",
    "standard": "^10.0.3",
    "tape": "^4.9.0",
    "uglify-js": "^3.3.23",
    "uglifyify": "^5.0.0",
    "unassertify": "^2.1.1",
    "watchify": "^3.11.0",
    "webdriverio": "^4.12.0"
  },
  "lint-staged": {
    "*.js": "standard"
  },
  "private": true,
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:css": "node-sass --output-style compressed styles/styles.sass > build/styles.css",
    "build:js": "browserify -p common-shakeify -p [ browserify-header --file header.js ] index.js | uglifyjs --comments '/license/' > build/browser.js",
    "chromedriver": "chromedriver --url-base=/wd/hub",
    "licenses": "licensee --production --errors-only | tee licenses.log",
    "lint": "standard",
    "precommit": "lint-staged",
    "test": "tape test/*.test.js",
    "prewatch:css": "npm run build:css",
    "watch:css": "node-sass --watch --source-map true --output build styles/styles.sass",
    "watch:js": "watchify -p common-shakeify --ignore-transform uglifyify --ignore-transform unassertify index.js -o 'exorcist build/browser.js.map > build/browser.js' -dv"
  }
}
